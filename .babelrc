{
    "presets": [
        "@babel/preset-env", 
        "@babel/preset-react"
    ],
    "plugins": [
        "@babel/plugin-proposal-class-properties",
        "@babel/plugin-proposal-object-rest-spread",
        "@babel/transform-runtime",
        [ "babel-plugin-direct-import",
            { "modules": ["@mui/material", "@mui/icons-material"] }
        ]
    ]
}